  Array.prototype.forEach.call(tabs, function(tab) {\n                tab.classList.remove('active');\n            });\n            \n            evt.currentTarget.classList.add('active');\n        }\n    \n        function showTab(selected, total)\n        {\n            var i;\n          for(i = 1; i <= total; i += 1)\n          {\n            document.getElementById('tabs-' + i).style.display = 'none';\n            document.getElementById('tabs' + i).style.fontWeight = 400; \n          }\n          document.getElementById('tabs-' + selected).style.display = 'block';\n          document.getElementById('tabs' + selected).style.fontWeight = 600;\n        }\n        \n        // fix back button error by keeping track of how many tab toggles there have been\n        // and then go back that number of pages to get back to home\n        //https://stackoverflow.com/questions/22641154/javascript-going-back-multiple-times-in-history\n\n            \n\n        return (\n            <div class=\"Panel\">\n                <nav>\n                    <ul class=\"Tabs\">\n                        <li class=\"Tabs__tab active Tab\" id=\"tabs1\" onClick={() => { showTab(1,2);}}><a href=\"javascript:;\">Solar</a></li>\n                        <li class=\"Tabs__tab Tab\" id=\"tabs2\" onClick={() => { showTab(2,2);}}><a href=\"javascript:;\">Solar + Battery</a></li>\n                        <hr id=\"tab-underline\"></hr>\n                        <li class=\"Tabs__presentation-slider\" role=\"presentation\"></li>\n                    </ul>\n                </nav>\n\n                \n\n                <p id=\"solar-subtext\"> I don’t own a home battery </p>\n                <p id=\"battery-subtext\"> I own a home battery </p>\n\n                <p id=\"form-desc\"> The information you provide will help determine the best solar system specifications for your home.</p>\n                <p id=\"form-desc2\"> In the case where you are not able to retrieve a field, we will use a default value for the analysis.</p>\n\n                <p class=\"Panel__body tabContent\" id=\"tabs-1\"> Solar Form </p>\n                <p class=\"Panel__body tabContent\" id=\"tabs-2\"> Solar + Battery Form </p>\n\n            </div>\n        )\n    }\n}\n\nexport default startSaving","/Users/tonili/Desktop/fydp-app/src/components/Navbar.js",[],"/Users/tonili/Desktop/fydp-app/src/components/Footer.js",["46","47"],"import React, { Component } from 'react';\n\nclass Footer extends Component {\n\n    \n    render() {\n        return (\n            <div class=\"footer\">\n                <p class=\"footer-left\">\n                    <a>SoliⒸ2020</a> &nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;\n                    <a>Management Engineering Capstone Design Project</a>\n                </p>\n            </div>\n\n        );\n\n    }\n}\n    \nexport default Footer",{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":16,"column":10,"nodeType":"54","messageId":"55","endLine":16,"endColumn":21},{"ruleId":"52","severity":1,"message":"56","line":2,"column":10,"nodeType":"54","messageId":"55","endLine":2,"endColumn":24},{"ruleId":"57","severity":1,"message":"58","line":44,"column":102,"nodeType":"59","endLine":44,"endColumn":125},{"ruleId":"60","severity":1,"message":"61","line":44,"column":110,"nodeType":"62","messageId":"63","endLine":44,"endColumn":124},{"ruleId":"57","severity":1,"message":"58","line":45,"column":95,"nodeType":"59","endLine":45,"endColumn":118},{"ruleId":"60","severity":1,"message":"61","line":45,"column":103,"nodeType":"62","messageId":"63","endLine":45,"endColumn":117},{"ruleId":"57","severity":1,"message":"64","line":10,"column":21,"nodeType":"59","endLine":10,"endColumn":24},{"ruleId":"57","severity":1,"message":"64","line":11,"column":21,"nodeType":"59","endLine":11,"endColumn":24},"no-native-reassign",["65"],"no-negated-in-lhs",["66"],"no-unused-vars","'currentTime' is assigned a value but never used.","I